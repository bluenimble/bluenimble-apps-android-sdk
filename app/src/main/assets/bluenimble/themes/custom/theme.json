{

  "*": {

    "primary": {
      "padding": "20 20 20 20",
      "text": {
        "align": "center middle",
        "color": "#FFFFFF",
        "shadow": "#7F000000 1 1 2"
      },
      "background": {
        "color": "#09afdf"
      }
    },

    "secondary": {
      "padding": "20 20 20 20",
      "text": {
        "align": "center middle",
        "color": "#FFFFFF",
        "shadow": "#7F000000 1 1 2"
      },
      "background": {
        "color": "#8bc43f"
      }
    },

    "logo": {
      "align": "middle",
      "margin": "? ? ? 20",
      "size": { "width": "500" }
    },

    "title": {
      "align": "right middle",
      "margin": "? 10 ? 10",
      "text": {
        "size": "14"
      }
    },

    "icon": {
      "size": { "width": "200", "height": "200" },
      "text": {
        "font": "fontawesome",
        "color": "#FFFFFF",
        "size": "24",
        "shadow": "#7F000000 1 1 2",
        "align": "center middle"
      },
      "background": {
        "color": "#FFFFFF #09afdf",
        "gradient": {
          "radius": "300"
        },
        "opacity": ".4"
      },
      "shadow": {
        "color": "#0293BD",
        "tick": "3"
      },
      "align": "center"
    },

    "icon-title": {
      "padding": "10 ? ? ?",
      "text": {
        "color": "#FFFFFF",
        "shadow": "#7F000000 1 1 2",
        "size": "16"
      }
    },

    "header": {
      "lineHeight": "all",
      "size": { "height": "200" },
      "background": {
        "color": "#FFFFFF"
      },
      "shadow": {
        "color": "#00aff0",
        "tick": "0 0 5 0"
      }
    },
    "main": {
      "margin": "200 0 0 0",
      "background": { "image": "background.jpg" }
    },

    "menuTpl": {
      "size": {
        "width": 340,
        "height": 340
      },
      "margin": "60 60 60 60"
    }

  }

}