{

  "*": {

    "logo": {
      "align": "middle",
      "margin": "? 40 ? 40"
    },

    "title": {
      "align": "middle",
      "text": {
        "size": "16",
        "color": "#FFFFFF"
      }
    },

    "icon": {
      "padding": "6 6 6 6",
      "text": {
        "font": "fontawesome",
        "size": "20",
        "color": "#FFFFFF"
      }
    },

    "header": {
      "compact": "true",
      "size": { "height": "200" },
      "background": {
        "color": "#00aff0 #00cdf0",
        "gradient": {
          "orientation": "bt"
        }
      },
      "shadow": {
        "color": "#00acf0",
        "tick": "5 0 0 0"
      }
    },
    "main": {
      "margin": "200 0 0 0",
      "background": { "color": "#FFFFFF" }
    },

    "menuTpl": {
      "size": {
        "width": 200,
        "height": 200
      }
    }

  }

}